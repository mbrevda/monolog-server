{
    "name": "mbrevda/log-server",
    "description": "A simple and lightweight log server that receives and displays logs from Monolog. Useful during development.",
    "authors": [
        {
            "name": "Moshe Brevda",
            "email": "mbrevda@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "youngj/httpserver": "dev-master",
        "aura/cli-kernel": "2.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Mbrevda\\MonologServer\\_Config\\": "config/",
            "Mbrevda\\MonologServer\\": "src/"
        }
    },
    "extra": {
      "aura": {
          "type": "library",
          "config": {
              "common": "Mbrevda\\MonologServer\\_Config\\Common"
          }
      }
  }
}
